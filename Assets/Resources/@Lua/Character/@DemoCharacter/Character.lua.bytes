local DemoSkillCharacter = _MOE.class("DemoSkillCharacter", _MOE.SkillManagerBase)

function DemoSkillCharacter:OnRegisterStateClass()
    local SkillNames = {
        "Locomotion"
    }
    self:RegisterSkills(SkillNames, true)
end

function DemoSkillCharacter:_OnPlayerInputMove(actionName, CallbackContext)
    _MOE.Logger.Log("[DemoSkillCharacter] _OnPlayerInputMove")
    self:OnPlayerInputEvent(actionName, CallbackContext)-- 发送到针对状态
end

function DemoSkillCharacter:OnInit()
    if _MOE.LuaPlayerInput then
        _MOE.LuaPlayerInput:RegisterLuaActionCallBack("Move", self._OnPlayerInputMove, self)
    end
    self:ChangeState("Locomotion")
end

function DemoSkillCharacter:OnUnInit()
    if _MOE.LuaPlayerInput then
        _MOE.LuaPlayerInput:UnRegisterLuaActionCallBack("Move")
    end
end

return DemoSkillCharacter