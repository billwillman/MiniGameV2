local _M = _MOE.class("DsPlayerModel", _MOE.BaseModel)

local function onServerStrEvent(eventName, strParam)
    _MOE.Logger.LogFormat("[DSPlayerController] eventName: {0} param: {1}", eventName, strParam)
end

function _M:RegisterPlayerEvents(PlayerController)
    if not PlayerController then
        return false
    end
    self:RegsterCSharpEvent(PlayerController, "onServerStrEvent", onServerStrEvent)
    return true
end

function _M:UnRegisterPlayerEvents(PlayerController)
    if not PlayerController then
        return false
    end
    self:UnRegsterCSharpEvent(PlayerController, "onServerStrEvent")
    return true
end

return _M