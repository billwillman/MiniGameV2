local _M = _MOE.class("DsPlayerModel", _MOE.BaseModel)

local function onServerStrEvent(eventName, strParam)
    _MOE.Logger.LogFormat("[DSPlayerController] eventName: {0} param: {1}", eventName, strParam)
end

function _M:RegisterPlayerEvents(LuaPlayerController)
    if not LuaPlayerController then
        return false
    end
    self:RegsterCSharpEvent(LuaPlayerController.PlayerController, "onServerStrEvent", onServerStrEvent)
    return true
end

function _M:UnRegisterPlayerEvents(LuaPlayerController)
    if not LuaPlayerController then
        return false
    end
    self:UnRegsterCSharpEvent(LuaPlayerController.PlayerController, "onServerStrEvent")
    return true
end

return _M